{{#if item.setImages.length}}
  {{#slick-carousel class="ember-carousel" autoplay=true autoplaySpeed=1500 speed=300 swipeToSlide=true infinite=false mobileFirst=true touchMove=true slidesToShow=1 slidesToScroll=1}}
    {{#each item.setImages as |image|}}
      <div data-src={{image}} class="imageZoom" >
        {{#if isMobileApp}}
          <img src={{image}} {{action "imageZoom" image}} class="profile slide-image"/>
        {{else}}
          <img src={{image}} class="profile"/>
        {{/if}}
      </div>
    {{/each}}
  {{/slick-carousel}}
{{else}}
  {{partial 'items/no_image'}}
{{/if}}
